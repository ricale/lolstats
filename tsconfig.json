{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es6",
      "jsx": "react",
  
      "resolveJsonModule": true,
  
      // "typeRoots": ["./src/@types", "./node_modules/@types"],
  
      "baseUrl": "./",
      "moduleResolution": "node",
      "paths": {
        "api": ["src/api"],
        "assets": ["src/assets"],
        "consts": ["src/consts"],
        // "components": ["src/components"],
        // "env": ["src/env"],
        // "logger": ["src/logger"],
        "pages": ["src/pages"],
        // "res": ["src/res"],
        "router": ["src/router"],
        "store": ["src/store"],
        "themes": ["src/themes"],
        "utils": ["src/utils"],
      },
  
      "allowSyntheticDefaultImports": true
    },
  }